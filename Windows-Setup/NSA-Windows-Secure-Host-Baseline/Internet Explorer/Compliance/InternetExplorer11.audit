# DISA STIG: Internet Explorer 11 version 1 release 10 
# Author: NSA Information Assurance
# Source: https://github.com/iadgov/Secure-Host-Baseline/Internet Explorer/Compliance/InternetExplorer11.audit

<check_type : "Windows" version : "2">
<group_policy: "Internet Explorer (IE) 11 Audit Policy">

#--------------------------------------#
#  [Internet Explorer] settings
#--------------------------------------#


<if>
<condition type: "or">

<custom_item> 
	type: FILE_VERSION
	description: "IE Version"
	info: "Running the latest version of IE protects users from known vulnerabilies."
	value_type: POLICY_FILE_VERSION
	value_data: "11.0"       
	file: "%programfiles% (x86)\Internet Explorer\iexplore.exe"
	check_type: CHECK_GREATER_THAN_OR_EQUAL
</custom_item>
<custom_item> 
	type: FILE_VERSION
	description: "IE Version"
	info: "Running the latest version of IE protects users from known vulnerabilies."
	value_type: POLICY_FILE_VERSION
	value_data: "11.0"       
	#file: "C:\Program Files\Internet Explorer\iexplore.exe"
	file: "%programfiles%\Internet Explorer\iexplore.exe"
	check_type: CHECK_GREATER_THAN_OR_EQUAL
</custom_item>

</condition>
<then>

<custom_item> 
	type: FILE_VERSION
	description: "IE 11 Version Check"
	info: "Running the latest version of IE protects users from known vulnerabilies."
	value_type: POLICY_FILE_VERSION
	value_data: "11.0"       
	file: "%programfiles%\Internet Explorer\iexplore.exe"
	check_type: CHECK_GREATER_THAN_OR_EQUAL
</custom_item>


