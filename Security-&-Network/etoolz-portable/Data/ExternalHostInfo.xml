<?xml version="1.0" encoding="utf-8"?>
<ExternalHostInfo>

	<Item>
		<Title>skittel.de (IP + Hostname)</Title>
		<URL>http://www.script.skittel.de/common/ip_hostname.php</URL>
		<IPAddress>
			<RegEx>^(\d+\.\d+\.\d+\.\d+):</RegEx>
			<SubPattern>1</SubPattern>
		</IPAddress>
		<Hostname>
			<RegEx>:(.+)$</RegEx>
			<SubPattern>1</SubPattern>
		</Hostname>
	</Item>

	<Item>
